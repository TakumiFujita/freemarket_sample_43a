= render 'shared/header'
%main.onsale
  .onsale__content
    .onsale__content__trade
      %h2 出品した商品
      %ul.onsale__content__trade__tabs
        %li.onsale__content__trade__tabs__during
          %h3
            %a{"data-toggle" => "tab", :href => "#mypage-tab-transaction-now"}
              出品中
        %li.onsale__content__trade__tabs__past
          %h3
            %a{"data-toggle" => "tab", :href => "#mypage-tab-transaction-now"}
              取引中
        %li.onsale__content__trade__tabs__past
          %h3
            %a{"data-toggle" => "tab", :href => "#mypage-tab-transaction-now"}
              売却済み
      .onsale__content__trade__content
        %ul.onsale__content__trade__content__during
          %li.onsale__content__trade__content__during__list
            出品中の商品がありません
        %ul.onsale__content__trade__content__past
          %li.onsale__content__trade__content__past__list
  = render 'shared/mypage-side-nav'
= render 'shared/footer'
= render 'shared/sellbtn'
